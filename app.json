{
	"expo": {
		"name": "SnugStash",
		"slug": "snugstash",
		"version": "0.0.1",
		"orientation": "portrait",
		"icon": "./assets/app-logo/icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/app-logo/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "240 10% 3.9%"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/app-logo/adaptive-icon/foreground.png",
				"backgroundColor": "./assets/app-logo/adaptive-icon/background.png"
			},
			"softwareKeyboardLayoutMode": "pan"
		},
		"web": {
			"bundler": "metro",
			"output": "single",
			"favicon": "./assets/app-logo/favicon.png"
		},
		"plugins": ["expo-router", "expo-secure-store"],
		"experiments": {
			"typedRoutes": true,
			"reactCompiler": true
		}
	}
}
